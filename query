SELECT date,time, title,status, locations.location,category.name as category,users.name 
from events join locations on events.location_id = locations.location_id
join category on category.category_id = events.category_id 
join assignments on assignments.event_id = events.event_id
join users on assignments.user_id = users.user_id
Where extract(month from events.date)=1
